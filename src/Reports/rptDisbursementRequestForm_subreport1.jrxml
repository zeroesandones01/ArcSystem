	<queryString>
		<![CDATA[SELECT FORMAT('%s / %s', fn_get_div_alias(a.div_id), fn_get_exec_ofc_alias(b.exec_office_code)) as cost_center
, c.acct_name
, fn_get_proj_alias(a.proj_id) AS proj_alias
, a.amount
, FORMAT('%s , %s', a.invoice_no, a.invoice_date::DATE) AS invoice
, FORMAT('%s , %s', a.soa_bill_no, a.soa_bill_date::DATE) AS soa_bill
FROM rf_drf_detail a
LEFT JOIN mf_division b ON b.div_code = a.div_id
LEFT JOIN mf_boi_chart_of_accounts c ON c.acct_id = a.acct_id AND c.status_id = 'A'
WHERE a.co_id = $P{co_id}
AND a.drf_no = $P{drf_no}]]>
	</queryString>
