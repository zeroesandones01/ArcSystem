	<queryString>
		<![CDATA[SELECT b.company_name
, b.company_address
, UPPER(e.pc_req_type_desc) as request_type
, fn_get_entity_name(a.payee) AS payee
, fn_get_div_alias(g.div_code) AS div_alias
, (CASE WHEN a.pcr_type IN ('01', '03') THEN a.pcr_no ELSE null END) AS ca_pcr_no
, d.status_desc as pcr_status
, TO_CHAR(a.pcr_date_created, 'Mon-dd-yyyy') AS date_requested
, (CASE WHEN a.pcr_type NOT IN ('01') THEN a.pcr_no ELSE null END) AS pcr_no
, COALESCE(a.cash_returned, 0.00) AS cash_returned
, COALESCE(a.amount_to_be_reimbursed, 0.00) AS amount_to_be_reimbursed
, COALESCE(a.pcr_total_amt, 0.00) AS ca_total_amt
FROM rf_petty_cash_header a
LEFT JOIN mf_company b ON b.co_id = a.co_id AND b.status_id = 'A'
LEFT JOIN rf_entity c ON c.entity_id = b.co_entity_id AND c.status_id = 'A'
LEFT JOIN mf_record_status d ON d.status_id = a.status_id
LEFT JOIN mf_petty_cash_req_type e ON e.pc_req_id = a.pcr_type AND e.status_id = 'A'
LEFT JOIN rf_employee g ON g.entity_id = a.payee AND g.status_id = 'A'
WHERE a.rec_status = 'A'
AND a.co_id = $P{co_id}
AND a.pcr_no = $P{pcr_no}]]>
	</queryString>
